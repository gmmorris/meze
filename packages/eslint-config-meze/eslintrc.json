{
  "plugins": [
      "react"
  ],
  "settings": {
    "react": {
      "pragma": "Meze"
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-unused-vars": ["error", { "varsIgnorePattern": "^Meze$" }]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  }
}
